{
  "create": {
    "required": [
      "name",
      "project_key"
    ],
    "fields": {
      "name": "string:Version name (e.g., '1.0.0', 'v2.1')",
      "project_key": "string:Project key the version belongs to",
      "description": "string:optional:Version description",
      "release_date": "string:optional:Planned release date",
      "archived": "boolean:optional:Whether version is archived",
      "released": "boolean:optional:Whether version is released"
    },
    "examples": {
      "minimal": {
        "name": "1.2.0",
        "project_key": "PROJ"
      },
      "complete": {
        "name": "2.0.0 - Authentication Update",
        "project_key": "PROJ",
        "description": "Major update including OAuth 2.0 authentication",
        "release_date": "2024-02-15",
        "archived": false,
        "released": false
      }
    },
    "cache_key": "",
    "cache_hint": ""
  },
  "update": {
    "required": [],
    "fields": {
      "name": "string:optional:Updated version name",
      "description": "string:optional:Updated description",
      "release_date": "string:optional:Updated release date",
      "archived": "boolean:optional:Archive status",
      "released": "boolean:optional:Release status"
    },
    "examples": {
      "minimal": {
        "released": true
      },
      "complete": {
        "name": "2.0.1 - Authentication Hotfix",
        "description": "Hotfix for OAuth integration issues",
        "released": true,
        "release_date": "2024-02-20"
      }
    },
    "cache_key": "",
    "cache_hint": ""
  },
  "get": {
    "required": [],
    "fields": {
      "expand": "string:optional:Expand options for version details"
    },
    "examples": {
      "minimal": {},
      "complete": {
        "expand": "operations,issuesStatusForFixVersion"
      }
    },
    "cache_key": "",
    "cache_hint": ""
  }
}