{
  "name": "mcp-atlassian-e2e",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "prep": "playwright install --with-deps",
    "seed": "uv run python seed/seed.py",
    "test": "REUSE_AUTH=true playwright test",
    "test:chromium": "REUSE_AUTH=true playwright test --project=chromium",
    "test:firefox": "REUSE_AUTH=true playwright test --project=firefox",
    "test:webkit": "REUSE_AUTH=true playwright test --project=webkit",
    "clean": "uv run python cleanup/cleanup.py",
    "auth": "node manual-login.js",
    "auth-setup": "node manual-login.js"
  },
  "devDependencies": {
    "@playwright/test": "^1.46.0",
    "dotenv": "^16.4.5"
  }
}
